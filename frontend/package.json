{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --tunnel",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "emulate": "firebase emulators:start --import=./../db",
    "export": "firebase emulators:export ./../db",
    "startlan": "expo start --lan",
    "postinstall": "cd .. && husky install frontend/.husky",
    "fix-hooks": "chmod +x .husky/pre-commit"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-masked-view/masked-view": "0.2.9",
    "@react-navigation/drawer": "^6.1.8",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "expo": "^49.0.13",
    "expo-file-system": "~15.4.4",
    "expo-intent-launcher": "~10.7.0",
    "expo-mail-composer": "~12.3.0",
    "expo-status-bar": "~1.6.0",
    "firebase": "^9.6.5",
    "intl": "^1.2.5",
    "kill-port": "^2.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.5",
    "react-native-animated-bar": "^0.2.0",
    "react-native-csv": "^0.2.0",
    "react-native-drax": "^0.10.2",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-paper": "^4.12.0",
    "react-native-progress": "^5.0.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-reanimated-progress-bar": "^1.0.2",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-swiper": "^1.6.0",
    "react-native-vector-icons": "^9.1.0",
    "react-native-web": "~0.19.6",
    "svg": "^0.1.0",
    "use-state-with-callback": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-typescript": "^7.16.7",
    "@types/intl": "^1.2.0",
    "@types/papaparse": "^5.3.2",
    "@types/react": "~18.2.14",
    "@types/react-native-vector-icons": "^6.4.10",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-universe": "^11.0.0",
    "firebase-tools": "^10.1.4",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1",
    "typescript": "^5.1.3"
  },
  "private": true,
  "lint-staged": {
    "*.ts": "yarn run eslint --fix",
    "*.tsx": "yarn run eslint --fix",
    "*.d.ts": "yarn run eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "universe/native",
      "universe/shared/typescript-analysis"
    ],
    "rules": {
      "import/namespace": 0
    },
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx",
          "*.d.ts"
        ],
        "parserOptions": {
          "project": "./tsconfig.json"
        }
      }
    ]
  }
}
